---
import { Image } from "@astrojs/image/components";
const logos = ["/images/hm-rm.png"];
---

<section class="bg-[#EBD96B] my-[32px]">
  <div class="container mx-auto px-4 py-8">
    <div class="flex">
      <div class="marquee one">
        {
          logos.map((logo) => (
            <Image
              src={logo}
              alt="Girl"
              width={119}
              height={69}
              format="webp"
              class="pointer-events-none"
            />
          ))
        }
      </div>
    </div>
  </div>
</section>
